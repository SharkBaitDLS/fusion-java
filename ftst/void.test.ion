(use 'check')
(use 'fusion/void')

(check_equal (is_void (void))     true "0")
(check_equal (is_void (void 1))   true "1")
(check_equal (is_void (void 1 2)) true "2")

(check_equal (apply void []) (void) "apply")

(check_equal (is_void null)  false "is_void null")
(check_equal (is_void true)  false "is_void true")
(check_equal (is_void false) false "is_void false")

//(check_equal (is_void undef) false "is_void undef") // TODO FUSION-71 FIXME
